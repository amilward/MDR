package uk.co.mdc.model

import com.lucastex.grails.fileuploader.UFile

class Document {
	
	String name
	String description
	String contentType
	String fileName
	byte[] content
	
	

	
	
    static constraints = {
		name blank: false, nullable: false, size: 2..20
		description blank: false, nullable: false, size: 2..500
		content nullable: false
		
    }
}

